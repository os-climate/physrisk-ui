{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "semi": "off",
        "quotes": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "@no-undef": "off"
    },
    "ignorePatterns": ["*.ts*", "*.tsx"]
}